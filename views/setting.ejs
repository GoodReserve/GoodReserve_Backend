    <link rel='stylesheet' href='/stylesheets/materialize.css' />
    <script src="/javascripts/materialize.js"></script>
    <link rel='stylesheet' href="/stylesheets/common.css" />
    <link rel='stylesheet' href='/stylesheets/setting.css'/>
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <script src="/javascripts/common.js"></script>
    <script src="/javascripts/setting.js"></script>
  </head>
  <body>
  <nav>
  <div class="nav-wrapper MainDarkRed valign-wrapper">
  	<a href="/" class="restaurantName"><%= restaurantName %></a>
  </div>
  </nav>
  <div class="content">
  <div class="settingContent">
  <ul class="collapsible" data-collapsible="accordion">
